{
    "rules": {
        ".read": "auth != null",
        ".write": "auth != null",
        "stocks": {
            ".indexOn": [
                "status"
            ],
            "$stockId": {
                ".validate": "newData.hasChildren(['importDate', 'assetId', 'status'])",
                "qt_received": {
                    ".validate": "newData.isNumber()"
                },
                "qt_distributed": {
                    ".validate": "newData.isNumber()"
                },
                "qt_balance": {
                    ".validate": "newData.isNumber()"
                }
            }
        }
    }
}